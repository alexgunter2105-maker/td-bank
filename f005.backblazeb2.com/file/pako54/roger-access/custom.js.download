function PopWin(PageName, screenheight, screenwidth) {
	var WinPop = 'toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=no,width=' + screenwidth + ',height='+ screenheight;
	var ScrLeft = (screen.width-screenwidth) / 2;
	var ScrTop = (screen.height-screenheight) / 2;
	WinPop = WinPop + ',left=' + ScrLeft +',top='+ ScrTop;
	winname = window.open(PageName, 'MyWindow', WinPop);
}

function PopWinAt(PageName, windowName, screenheight, screenwidth, screenleft, screentop) {
	var WinPop = 'toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=no';
	var WinTitle = windowName;
	if(windowName.length < 1) { WinTitle = 'popWindow'; }
	WinPop = WinPop + ',width=' + screenwidth + ',height='+ screenheight;
	WinPop = WinPop + ',left=' + screenleft +',top='+ screentop;
	winname= window.open(PageName, WinTitle, WinPop);
}

