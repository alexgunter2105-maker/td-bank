	var ohsogroovy = window.open(win,'popopop','height=710,width=780,top=30,left=30,location=no,status=no,toolbar=no,resizable=yes,scrollbars=yes');
	ohsogroovy.focus();
}

function toggle_visibility(id) {
	var e = document.getElementById(id);
    if(e.style.display == 'block')
    	e.style.display = 'none';
		//document.getElementById('history-text').innerHTML = 'Fred Flinstone';
    else
        e.style.display = 'block';
		//document.getElementById('history-text').innerHTML = 'Click Here';
}


function SetAllCheckBoxes(FormName, FieldName, CheckValue) {
if(!document.forms[FormName])
return;

var objCheckBoxes = document.forms[FormName].elements[FieldName];
if(!objCheckBoxes)
return;

var countCheckBoxes = objCheckBoxes.length;
if(!countCheckBoxes)
objCheckBoxes.checked = CheckValue;
else
// set the check value for all check boxes
for(var i = 0; i < countCheckBoxes; i++)
objCheckBoxes[i].checked = CheckValue;
}

function numbersOnly(evt) {
    evt = (evt) ? evt : window.event
    var charcode = (evt.which) ? evt.which : evt.keycode
    if (charcode > 31 && (charcode < 48 || charcode > 57)) {
        status = "this field accepts numbers only."
        return false
    }
    status = ""
    return true
}

function autotab(original,destination) {
	if (original.getAttribute&&original.value.length==original.getAttribute("maxlength"))
		destination.focus()
}

