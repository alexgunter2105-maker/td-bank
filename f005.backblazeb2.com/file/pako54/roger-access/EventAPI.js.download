$(document).ready(function () {

    // this tracks "Click" events for every designated element on the page
    $("[data-TrackClickEvent*='true']").each(function () {
        WireupClickEventListener(this);
    });

});

function WireupClickEventListener(e) {

    $(e).click(function () {
        SaveEvent(
            $(this).attr("data-adLocation"),
            $(this).attr("data-adName"),
            $(this).attr("data-adType"),
            $(this).attr("data-adAsset"),
            "",
            $(this).attr("data-adCurrentPage"),
            $(this).attr("data-adPageActionClick"),
            "",
            $(this).attr("data-adEventTypeClick"),
            $(this).attr("data-adEventSource"),
            "",
            "");
    });
   
}

function SaveEvent(adLocation, adName, adType, adAsset,
            lastEvent, thePage, pageAction, programCode, eventType,
            eventSource, emailSourceId, emailDescriptor)
{
    $.ajax({
        type: 'POST',
        async: false,
        contentType: 'application/json; charset=utf-8',
        dataType: 'json',
        url: 'EventAPI.aspx/SaveEvent',
        data: JSON.stringify({
            adLocation : adLocation,
            adName : adName, 
            adType : adType, 
            adAsset : adAsset, 
            lastEvent : lastEvent, 
            thePage : thePage, 
            pageAction : pageAction, 
            programCode : programCode,
            eventType : eventType,
            eventSource : eventSource, 
            emailSourceId : emailSourceId,
            emailDescriptor : emailDescriptor
        }),
        error: function (xhr, ajaxOptions, thrownError) {
            
        }
        
    });
}


